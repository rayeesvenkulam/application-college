<main id="main" class="main">

  <div class="pagetitle">
    <h1>Dashboard</h1>
    <nav>
      <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="index.html">Home</a></li>
        <li class="breadcrumb-item active">Dashboard</li>
      </ol>
    </nav>
  </div><!-- End Page Title -->

  <section class="section profile">
    <div class="row">
      <div class="col-xl-4">

        <div class="card">
          <div class="card-body profile-card pt-4 d-flex flex-column align-items-center">
            {{!-- <img src="assets/img/profile-img.jpg" alt="Profile" class="rounded-circle"> --}}
            <h2>{{userName}}</h2>
            <h3>Student</h3>
            <div class="social-links mt-2">
              <a href="#" class="twitter"><i class="bi bi-twitter"></i></a>
              <a href="#" class="facebook"><i class="bi bi-facebook"></i></a>
              <a href="#" class="instagram"><i class="bi bi-instagram"></i></a>
              <a href="#" class="linkedin"><i class="bi bi-linkedin"></i></a>
            </div>
          </div>
        </div>

      </div>

      <div class="col-xl-8">

        <div class="card">
          <div class="card-body pt-3">
            <!-- Bordered Tabs -->

            <div class="tab-content ">

              <div class="tab-pane fade show active profile-overview" id="profile-overview" role="tabpanel">

                <h5 class="card-title">My Profile</h5>

                <div class="row">
                  <div class="col-lg-3 col-md-4 label ">Full Name</div>
                  <div class="col-lg-9 col-md-8">{{data.name}}</div>
                </div>

                <div class="row">
                  <div class="col-lg-3 col-md-4 label">Branch & Semester</div>
                  <div class="col-lg-9 col-md-8">{{data.branch }} - {{data.semester }}</div>
                </div>

                <div class="row">
                  <div class="col-lg-3 col-md-4 label">Adm No</div>
                  <div class="col-lg-9 col-md-8">{{data.adm_number }}</div>
                </div>

                <div class="row">
                  <div class="col-lg-3 col-md-4 label">Adm Year</div>
                  <div class="col-lg-9 col-md-8">{{data.adm_year }}</div>
                </div>

                <div class="row">
                  <div class="col-lg-3 col-md-4 label">Quota</div>
                  <div class="col-lg-9 col-md-8">{{data.quota }}</div>
                </div>

                <div class="row">
                  <div class="col-lg-3 col-md-4 label">University Reg.No</div>
                  <div class="col-lg-9 col-md-8">{{data.university_no}}</div>
                </div>

                <div class="row">
                  <div class="col-lg-3 col-md-4 label">Mobile No</div>
                  <div class="col-lg-9 col-md-8">{{data.mobile1}} , {{data.mobile2}}</div>
                </div>

              </div>



            </div><!-- End Bordered Tabs -->

          </div>
        </div>

      </div>
    </div>
  </section>
</main><!-- End #main -->